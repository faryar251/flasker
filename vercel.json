{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        },
        {
            "src": "/static/**",
            "use": "@vercel/static"
        },
        {
            "src": "/templates/**",
            "use": "@vercel/templates"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    
        "rewrites": [{ "source": "/(.*)", "destination": "/base.html" }]
    
}
